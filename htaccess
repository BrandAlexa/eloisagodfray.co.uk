<FilesMatch "\.(jpg|jpeg|png|webp|mp4|webm|svg)$">
Header set Cache-Control "public, max-age=1928448000"
</FilesMatch>

<FilesMatch "\.(html|css|js)$">
Header set Cache-Control "public, max-age=0, no-cache, no-store, must-revalidate"
</FilesMatch>

<Files .htaccess>
order allow,deny
deny from all
</Files>
